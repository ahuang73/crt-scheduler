{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "server"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "target":"ES2022",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
  }
}
